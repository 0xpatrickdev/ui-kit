// This file can contain .js-specific Typescript compiler config.
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "noEmit": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "jsx": "react",
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "test/**/*.js",
  ],
}
